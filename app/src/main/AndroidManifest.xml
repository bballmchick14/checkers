<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="edu.msu.team23.checkers">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_spartycheckers"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_spartycheckers_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="edu.msu.team23.checkers.GameOverActivity" />
        <activity android:name="edu.msu.team23.checkers.CheckersActivity" />
        <activity android:name="edu.msu.team23.checkers.MainActivity" android:launchMode="singleTop">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>